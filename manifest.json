{
  "manifest_version": 3,
  "name": "Top Academy Dark Theme",
  "version": "0.1",
  "description": "This extension adds dark theme to Top Academy Journal",
  "content_scripts": [{
    "css": ["journal/style.css"],
    "matches": ["https://journal.top-academy.ru/*"]
  }],
  "action": {
    "default_popup": "extension.html",
    "default_icon": "icons/extension_icon.png"
  }
}